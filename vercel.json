{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/\\.well-known/(.*)",
      "dest": "/public/.well-known/$1"
    },
    {
      "src": "/legal/(.*)",
      "dest": "/public/legal/$1"
    },
    {
      "src": "/(status|contact|request)",
      "dest": "/server.js"
    }
  ]
}