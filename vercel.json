{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "ai-plugin.json", "use": "@vercel/static" },
    { "src": ".well-known/openapi.yaml", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/ai-plugin.json", "dest": "/ai-plugin.json" },
    { "src": "/\\.well-known/openapi.yaml", "dest": "/.well-known/openapi.yaml" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}
