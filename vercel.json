{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/\\.well-known/openapi\\.yaml",
      "dest": "/public/.well-known/openapi.yaml"
    },
    {
      "src": "/legal/privacy",
      "dest": "/public/legal/privacy.html"
    },
    {
      "src": "/legal/terms",
      "dest": "/public/legal/terms.html"
    },
    { "src": "/status", "dest": "/server.js" },
    { "src": "/contact", "dest": "/server.js" },
    { "src": "/request", "dest": "/server.js" }
  ]
}
